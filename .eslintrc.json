{
  "env": {
    "browser": true,
    "es2021": true,
    "jasmine": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "no-console": ["warn", { "allow": ["warn", "error", "log"] }],
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "prefer-arrow-callback": "off",
    "func-names": "off",
    "object-shorthand": "warn",
    "no-param-reassign": ["error", { "props": false }],
    "import/extensions": "off"
  }
}
